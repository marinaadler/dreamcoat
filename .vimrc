set nocompatible
if has('filetype')
    filetype on
    filetype plugin on
    filetype indent on
endif
if has('syntax')
    syntax on
endif
set hidden
set backspace=indent,eol,start
set ruler
set laststatus=2
set confirm
set visualbell
if has('mouse')
    set mouse=a
endif
set cursorline
set cursorcolumn
set number
set relativenumber
set shiftwidth=4
set tabstop=4
set expandtab
set nobackup
set scrolloff=10
set nowrap
set incsearch
set ignorecase
set smartcase
set showcmd
set showmode
set showmatch
set hlsearch
set history=1000
set wildmenu
set wildmode=list:longest
set wildignore=*.docx,*.jpg,*.png,*.gif,*.pdf,*.pyc,*.exe,*.flv,*.img,*.xlsx
set colorcolumn=89
